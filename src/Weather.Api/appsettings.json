{
  "ConnectionStrings": {
    "Forecast": "__ConnectionStrings:Forecast__"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "Authentication": {
    "Schemes": {
      "Bearer": {
        "Authority": "__Authentication:Schemes:Bearer:Authority__",
        "ValidAudiences": [ "__Authentication:Schemes:Bearer:ValidAudiences:0__" ],
        "ValidIssuer": "__Authentication:Schemes:Bearer:ValidIssuer__"
      }
    },
    "Swagger": {
      "AuthorizationUrl": "__Authentication:Swagger:AuthorizationUrl__",
      "TokenUrl": "__Authentication:Swagger:TokenUrl__",
      "RefreshUrl": "__Authentication:Swagger:RefreshUrl__",
      "Scopes": [ "__Authentication:Swagger:Scopes:0__" ],
      "ClientId": "swagger",
      "ClientSecret": "__Authentication:Swagger:ClientSecret__"
    }
  },
  "AllowedHosts": "*",
  "OutboxMessageProcessor": {
    "Period": "00:00:30",
    "MaximumConcurrentMessage": 10000
  }
}
